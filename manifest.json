{
  "manifest_version": 3,
  "name": "PPT",
  "version": "1.0",
  "description": "Mengubah URL tertentu menjadi URL yang diinginkan.",
  "permissions": ["tabs", "activeTab"],
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
